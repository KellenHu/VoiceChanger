<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".anfixdemo.AndfixMainActivity">


    <Button
        android:id="@+id/start_self"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:text="启动自己"
        app:layout_constraintBottom_toTopOf="@+id/start_mainactivity"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/start_mainactivity"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="384dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:text="启动MainActivity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/start_self"
        tools:ignore="MissingConstraints" />
</android.support.constraint.ConstraintLayout>